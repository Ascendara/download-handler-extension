{
  "manifest_version": 3,
  "name": "Ascendara Download Handler",
  "version": "2.2",
  "description": "Redirects downloads from specific file hosting websites to the Ascendara Download Manager for faster downlaoding.",
  "permissions": [
      "downloads",
      "storage"
  ],
  "host_permissions": [
      "*://*.megadb.xyz/*",
      "*://*.datanodes.to/*",
      "*://*.qiwi.gg/*",
      "*://*.buzzheavier.com/*"
  ],
  "icons": {
      "16": "icons/ascendara-16.png",
      "32": "icons/ascendara-32.png",
      "48": "icons/ascendara-48.png",
      "128": "icons/ascendara-128.png"
  },
  "background": {
      "service_worker": "background.js"
  },
  "action": {
      "default_popup": "popup.html",
      "default_icon": {
          "16": "icons/ascendara-16.png",
          "48": "icons/ascendara-48.png",
          "128": "icons/ascendara-128.png"
      }
  }
}
